      <!-- Begin: Content -->
      <section id="content" class="animated fadeIn">


        <!-- Demo Text Content -->
        <div class="pl15 pr50">
          {{outlet}}
        </div>

        <!-- Begin: Page Footer -->
        <footer id="content-footer" class="affix">
          <div class="row">
            <div class="col-md-6">
              <span class="footer-legal">Â© 2015 JPP</span>
            </div>
            <div class="col-md-6 text-right">
              <span class="footer-meta">Version: <b {{action "showReleaseNotes"}}>0.1</b></span>
            </div>
          </div>
        </footer>
        <!-- End: Page Footer -->

      </section>
      <!-- End: Content -->

    </section>

  </div>
  <!-- End: Main -->

